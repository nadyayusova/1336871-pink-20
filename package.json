{
  "name": "pink-20",
  "private": true,
  "description": "Личный проект «Пинк» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.8.4",
    "browser-sync": "2.26.7",
    "del": "^5.1.0",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-js-minify": "0.0.3",
    "gulp-less": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgstore": "^7.0.1",
    "gulp4-run-sequence": "^1.0.1",
    "posthtml-include": "^1.4.3",
    "gulp": "4.0.2",
    "stylelint": "13.6.1",
    "stylelint-config-htmlacademy": "0.1.4",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp clean && gulp sprite && gulp build && gulp clean:sprite",
    "start": "npm run build && gulp server"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "12.18.2"
  }
}
